{
  "compilerOptions": {
    "target": "es2022",
    "jsx": "preserve",
    "lib": ["DOM", "ESNext"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [
      "vite/client",
      "node",
      "vite-plugin-vue-server-ref/module"
    ]
  },
  "exclude": [
    "**/dist",
    "**/docs",
    "node_modules"
  ]
}
